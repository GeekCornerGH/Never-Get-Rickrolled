{
    "manifest_version": 2,
    "name": "Never Get Rickrolled",
    "description": "Don't be annoyed with rickrolls.",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": ["src/app.js"]
        }
    ],
    "web_accessible_resources": [
        "src/links.json",
        "src/translations.json",
        "page.html"
    ]
}